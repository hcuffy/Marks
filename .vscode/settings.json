{
  "files.associations": {
    ".babelrc": "jsonc",
    ".eslintrc": "jsonc",
    ".prettierrc": "jsonc",
    ".stylelintrc": "json",
    ".dockerignore": "ignore",
    ".eslintignore": "ignore",
    ".flowconfig": "ignore"
  },
  "javascript.validate.enable": false,
  "javascript.format.enable": false,
  "typescript.validate.enable": false,
  "typescript.format.enable": false,
  "editor.rulers": [119],
  "flow.useNPMPackagedFlow": true,
  "search.exclude": {
    ".git": true,
    ".eslintcache": true,
    "app/dist": true,
    "app/main.prod.js": true,
    "app/main.prod.js.map": true,
    "bower_components": true,
    "dll": true,
    "flow-typed": true,
    "release": true,
    "node_modules": true,
    "npm-debug.log.*": true,
    "test/**/__snapshots__": true,
    "yarn.lock": true
  },
  "cSpell.words": [
    "DROPDOWN",
    "Datastore",
    "Fbutton",
    "autoload",
    "camelcase",
    "classdata",
    "compat",
    "dropdown",
    "dropdowns",
    "dropup",
    "examid",
    "firstname",
    "lastname",
    "lifecycle",
    "parens",
    "prettierrc",
    "reactstrap",
    "sidemenu",
    "studentid",
    "stylelintrc",
    "subjectid",
    "subjectname",
    "textbox"
  ]
}
