	"build": {
		"productName": "Marks",
		"appId": "com.henry.marks",
		"files": [
			"dist/",
			"node_modules/",
			"app.html",
			"main.prod.js",
			"main.prod.js.map",
			"package.json"
		],
		"dmg": {
			"contents": [
				{
					"x": 130,
					"y": 220
				},
				{
					"x": 410,
					"y": 220,
					"type": "link",
					"path": "/Applications"
				}
			]
		},
		"mac": {
			"target": [
				"dmg",
				"mas",
				"zip"
			]
		},
		"win": {
			"target": [
				"nsis",
				"msi"
			]
		},
		"linux": {
			"target": [
				"deb",
				"rpm",
				"AppImage"
			],
			"category": "Development"
		},
		"directories": {
			"buildResources": "resources",
			"output": "release"
		},
		"publish": {
			"provider": "github",
			"owner": "hcuffy",
			"repo": "Marks",
			"private": false
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hcuffy/Marks.git"
	},
